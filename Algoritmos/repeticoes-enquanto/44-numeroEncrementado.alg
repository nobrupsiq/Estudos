algoritmo "numeroIncrementado"
var
vinicial, vfinal, incremento, i: inteiro
inicio

escreva ("Digite o valor INICIAL: ")
leia(vinicial)
escreva ("Digite o valor FINAL: ")
leia(vfinal)
escreva ("Digite o INCREMENTO: ")
leia(incremento)

i <- vinicial
enquanto (i < vfinal) faca
         escreva(i)
         i <- i + incremento
fimenquanto
escreval (" ACABOU!")
fimalgoritmo
